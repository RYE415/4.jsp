<script>
  $(function(){
	  //1.url요청문서 2.매개변수를 전달(생략이 가능) 3.콜백함수
	  $.ajax('getXml.jsp',{
		  //data:~매개변수가 없으면 생략
		  success:function(data){
			  //분리작업->(=파싱분리) parse->find() or filter()
			  $(data).find('person').each
		  }
	  })
  })
</script>
