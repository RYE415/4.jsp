<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

 <!-- typeAliases은 type=상위패키지명.하위패키지명...불러올 패키지명
 						(alias="별칭명"을 부여할 수 있는 태그)
 	  mappers=>접속 시 불러올 테이블에 대한 정보 xml파일을 지정해줄 때 사용하는 태그
 	  		   resource="경로 포함해서 불러올 xml 파일명(/)"
  -->
  
  <configuration>
  	<typeAliases>
  		<typeAlias type="krh.BoardCommand" alias="board" />
  	
  	</typeAliases>
  	<mappers>
  		<mapper resource="krh/Board.xml" />
  		<!-- 
  		<mapper resource="krh/Sangpum.xml" />
  		<mapper resource="krh/Member.xml" />
  		<mapper resource="krh/Notice.xml" />
  		,,,t
  		 -->
  	</mappers>
  </configuration>